{
  "name": "aws-access-parser",
  "version": "1.154.0",
  "description": "AWS ELB Access Log Parser and metric emitter",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "pretest": "npm run lint",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "pre:build": "npm cit",
    "all": "npm run build && npm prune --production && rm -f dist/lambda.zip && zip -q -r dist/lambda.zip node_modules dist && npm ci",
    "build": "npm run pre:build && tsc",
    "start": "node ./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mozilla/fxa.git"
  },
  "engines": {
    "node": ">=10"
  },
  "author": "Mozilla (https://mozilla.org/)",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/mozilla/fxa/issues"
  },
  "homepage": "https://github.com/mozilla/fxa#readme",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.40",
    "@types/chai": "^4.2.7",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.8",
    "@types/sinon": "^7.5.1",
    "chai": "^4.2.0",
    "mocha": "^7.0.0",
    "prettier": "^1.19.1",
    "sinon": "^8.1.0",
    "ts-node": "^8.6.2",
    "ts-sinon": "^1.0.24",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "aws-sdk": "^2.608.0",
    "lodash": "^4.17.15"
  }
}
